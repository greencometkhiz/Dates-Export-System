@model IEnumerable<scproject.CustomerSupplier>

@{
    ViewBag.Title = "IndexCustomerSupplier";
    Layout = "~/Views/Shared/_LayoutforAdmin.cshtml";
}

<h2>Export Customer Suppliers</h2>
<div>

  
    <div class="container" style="float:left;width: 400px; height: 300px; margin: 15px; background-color:transparent; color: white;">
        <img style="width: 400px; height: 300px;float:left;margin-left:0px;" src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSn9hz2xgTsElxJIvtaunuCiuof03i8xeZPtA&usqp=CAU" />

        <br />
        <br />
        <br />
        <br /><center>

        </center>
    </div>

    <div class="container" style="float: right; width: 400px; height: 300px; margin: 15px; background-color: transparent; color: white;">
        <img style="width: 400px; height: 300px;float:right;margin-right:0px;" src="data:image/jpeg;base64," />

        <br />
        <br />
        <br />
        <br /><center>
            <h2>Total Products:</h2>

        </center>
    </div>

</div>

<div>
    <p>
        <br /><br />
        <br />
        <br /><br />
        <br />
        <br /><br />
        <br />
        <br /><br />
        <br />
        <br />
        <br /><br />
        <br />
        <br />

        @using (Html.BeginForm("IndexCustomerSupplier", "Admin", FormMethod.Get))
        {
            <b>Search by:  </b>@Html.RadioButton("searchBy", "CSName", true)<text>Costumer Supplier Name</text>
            <br /><br />
            @Html.TextBox("search")<input type="submit" value="Search" style="background-color:darkred;color:white"/>
        }
    </p>
    <p>
        @Html.ActionLink("Create New", "CreateCustomerSupplier")
    </p>
    <table class="table" style="border-color:darkred;border:solid;">
        <tr style="background-color:darkred;color:white">
            <th>
                <h5><b>Customer Supplier ID</b></h5>
            </th>
            <th>
                <h5><b>Name</b></h5>
            </th>
            <th>
                <h5><b>Address</b></h5>
            </th>
            <th>
                <h5><b>Phone Number</b></h5>
            </th>
            <th>
                <h5><b>Email</b></h5>
            </th>
            <th>
                <h5><b>Payment ID</b></h5>
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.CSID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CSName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CSAddress)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PhoneNo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Emailad)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PaymentID)
                </td>
                <td>
                    @Html.ActionLink("Edit", "EditCustomerSupplier", new { id = item.CSID }) |
                    @Html.ActionLink("Details", "DetailsCustomerSupplier", new { id = item.CSID }) |
                    @Html.ActionLink("Delete", "DeleteCustomerSupplier", new { id = item.CSID })
                </td>
            </tr>
        }

    </table>
</div>